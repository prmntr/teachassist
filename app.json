{
  "expo": {
    "name": "TeachAssist",
    "slug": "teachassist",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "teachassist",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/playstore-icon.png",
        "backgroundColor": "#161616"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.prmntr.teachassist"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/transparent_ta_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/playstore-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#131313"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6e2397fc-f645-446d-ac3a-1db936c657e0"
      }
    },
    "owner": "prmntr",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6e2397fc-f645-446d-ac3a-1db936c657e0"
    }
  }
}
